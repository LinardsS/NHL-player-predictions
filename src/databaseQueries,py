import sqlite3
from utils import convertTimeStringToMinutes

conn = sqlite3.connect('main.db')

c = conn.cursor()

c.execute("SELECT * FROM skater_game_data LIMIT 1")

row = c.fetchall()[0]
print(row)
toi = row[6]
print(toi)
print("Converted toi: " + str(convertTimeStringToMinutes(toi)))
conn.commit()

conn.close()